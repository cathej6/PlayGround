<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/childmap"
    tools:context="edu.washington.cathej.playground.MapActivity">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageButton
            android:background="#0000"
            android:src="@drawable/blue_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:id="@+id/imageButton2"
            android:layout_marginTop="60dp"
            android:layout_alignTop="@+id/imageButton3"
            android:layout_centerHorizontal="true" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/grey_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="27dp"
            android:id="@+id/imageButton"
            android:layout_alignBottom="@+id/imageButton3"
            android:layout_toLeftOf="@+id/imageButton2"
            android:layout_toStartOf="@+id/imageButton2" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/blue_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:id="@+id/imageButton4"
            android:layout_below="@+id/imageButton3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="13dp" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/yellow_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:id="@+id/imageButton3"
            android:layout_marginTop="194dp"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/blue_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:id="@+id/imageButton5"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="22dp"
            android:layout_below="@+id/imageButton2"
            android:layout_alignLeft="@+id/imageButton2"
            android:layout_alignStart="@+id/imageButton2" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/grey_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:layout_marginBottom="34dp"
            android:id="@+id/imageButton6"
            android:layout_alignBottom="@+id/imageButton"
            android:layout_alignLeft="@+id/imageButton3"
            android:layout_alignStart="@+id/imageButton3" />

        <ImageButton
            android:background="#0000"
            android:src="@drawable/grey_f"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:layout_above="@+id/imageButton6"
            android:layout_toLeftOf="@+id/imageButton2"
            android:layout_toStartOf="@+id/imageButton2"
            android:id="@+id/imageButton7" />
    </RelativeLayout>
</FrameLayout>
